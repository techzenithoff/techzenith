<p id="notice"><%= notice %></p>

<h1>Tickets</h1>

<table>
  <thead>
    <tr>
      <th>Uid</th>
      <th>Slug</th>
      <th>Ticket type</th>
      <th>Name</th>
      <th>Event</th>
      <th>Price</th>
      <th>Available quantity</th>
      <th>Description</th>
      <th>Status</th>
      <th>Account</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tickets.each do |ticket| %>
      <tr>
        <td><%= ticket.uid %></td>
        <td><%= ticket.slug %></td>
        <td><%= ticket.ticket_type_id %></td>
        <td><%= ticket.name %></td>
        <td><%= ticket.event_id %></td>
        <td><%= ticket.price %></td>
        <td><%= ticket.available_quantity %></td>
        <td><%= ticket.description %></td>
        <td><%= ticket.status %></td>
        <td><%= ticket.account_id %></td>
        <td><%= link_to 'Show', ticket %></td>
        <td><%= link_to 'Edit', edit_ticket_path(ticket) %></td>
        <td><%= link_to 'Destroy', ticket, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket', new_ticket_path %>
